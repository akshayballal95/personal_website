{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.supportedFileTypes": [
    "md",
    "mdx",
    "markdown",
    "svx"
  ],
  "frontMatter.preview.host": "http://localhost:5173",
  "frontMatter.preview.pathName": "blogs",
  "frontMatter.framework.startCommand": "npm run dev",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "stage",
          "name": "stage",
          "type": "string"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        }
      ]
    },
    {
      "name": "blog",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "stage",
          "name": "stage",
          "type": "string"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "blogs",
      "path": "[[workspace]]/src/lib/assets/blogs"
    }
  ],
  "frontMatter.content.publicFolder": "",
  "frontMatter.framework.id": "other"
}